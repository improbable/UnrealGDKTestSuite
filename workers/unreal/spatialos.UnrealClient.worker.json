{
  "build": {
    "tasks_filename": "Game/Intermediate/Improbable/spatialos.unreal.client.build.json",
    "generated_build_scripts_type": "unreal"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "UnrealClient"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 50
      }
    },
    "streaming_query": [
      {
        "global_component_streaming_query": {
          "component_name": "improbable.unreal.UnrealLevel"
        }
      }
    ],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": true
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "${UNREAL_HOME}\\Engine\\Binaries\\Win64\\UE4Editor.exe",
        "arguments": [
            "${IMPROBABLE_WORKER_DIR}/Game/SampleGame.uproject",
            "-game",
            "+workerType",
            "UnrealClient",
			"-stdout",
			"-nowrite",
			"-unattended",
			"-nologtimes",
			"-nopause",
			"-noin",
			"-messaging",
			"-abslog=${IMPROBABLE_LOG_FILE}",
            "-NoVerifyGC",
			"-windowed",
			"-ResX=800",
			"-ResY=600"
        ]
      }
    }
  }
}
